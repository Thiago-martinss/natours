{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning Node, express and MongoDB",
  "main": "app.js",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "node server.js",
    "dev": "SET NODE_ENV=development&&nodemon server.js",
    "start:prod": "NODE_ENV=production node --watch server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch --no-hmr ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build:js": "parcel build  ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js"
  },
  "author": "Thiago",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.13.22",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "nodemailer": "^6.9.15",
    "nodemailer-brevo-transport": "^2.1.0",
    "pug": "^3.0.3",
    "save-dev": "^0.0.1-security",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "stripe": "^17.4.0",
    "validator": "^13.12.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "ndb": "^1.1.5",
    "parcel-bundler": "^1.12.3"
  },
  "engines": {
    "node": ">=14.x.x"
  }
}
